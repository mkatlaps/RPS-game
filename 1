human_turn = input('Ievadi savu RPS:')
computer_turn = 'scissors'

if human_turn == computer_turn:
    print('Neizskirts!')
elif human_turn == 'rock' and computer_turn == 'scissors':
    print('Tu uzvari!')
elif human_turn == 'paper' and computer_turn == 'rocs':
    print('Tu uzvari!')
elif human_turn == 'scissors' and computer_turn == 'paper':
    print('Tu uzvari!')
else:
    print('Tu zaude!')
